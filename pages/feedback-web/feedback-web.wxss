/* pages/feedback-web/feedback-web.wxss */
.nav{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 100rpx;
  border-bottom: 1px solid #eee;
  background-color: #fff;
}
.nav>view{
  flex: 1;
}
.title{
  text-align: center;
  color: #333;
}
.back-icon{
  padding-left: 10rpx;
  width: 80rpx;
}
.back-icon image{
  display: block;
  width: 52rpx;
  transform: rotateY(180deg);
}